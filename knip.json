{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/**/*.{js,ts,jsx,tsx}",
    "next.config.*",
    "tailwind.config.*",
    "postcss.config.*"
  ],
  "project": ["src/**/*.{js,ts,jsx,tsx}", "!src/**/*.d.ts"],
  "ignoreDependencies": [
    "next-auth",
    "pdf-lib",
    "react-hook-form",
    "zod",
    "zustand",
    "eslint-config-next",
    "tw-animate-css",
    "tailwindcss"
  ],
  "ignoreExportsUsedInFile": true,
  "includeEntryExports": true,
  "workspaces": {
    ".": {
      "entry": ["src/app/**/*.{js,ts,jsx,tsx}", "next.config.*"],
      "project": ["src/**/*.{js,ts,jsx,tsx}", "!src/**/*.d.ts"]
    }
  }
}
